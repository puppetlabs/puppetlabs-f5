					<xsd:attribute ref='SOAP-ENC:arrayType' wsdl:arrayType='tns:Management.KeyCertificate.CertificateRequestInformation[]'/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
