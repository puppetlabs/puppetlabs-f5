		</xsd:complexType>
		<xsd:simpleType name="GlobalLB.DNSSECKey.KeyType">
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="KEY_TYPE_UNKNOWN">
					<xsd:annotation>
						<xsd:documentation>KEY_TYPE_UNKNOWN</xsd:documentation>
