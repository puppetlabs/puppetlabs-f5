		<soap:operation soapAction="urn:iControl:Management/EM"/>
		<input>
			<soap:body
				use="encoded"
				namespace="urn:iControl:Management/EM"
				encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
		</input>
		<output>
			<soap:body
				use="encoded"
