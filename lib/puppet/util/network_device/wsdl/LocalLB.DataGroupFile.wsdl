	<operation name="get_list">
   	<documentation>
 Gets the identifiers of all data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_listRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_listResponse"/>
	</operation>
	<operation name="create">
	<documentation>
 Creates a set of data group file objects.

 Once the file is imported into the file cache, it is deleted.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.createRequest"/>
		<output message="tns:LocalLB.DataGroupFile.createResponse"/>
	</operation>
	<operation name="delete_data_group_file">
	<documentation>
 Deletes a set of data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.delete_data_group_fileRequest"/>
