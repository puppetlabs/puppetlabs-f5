	</operation>
	<operation name="delete_all_data_group_files">
	<documentation>
 Deletes all data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.delete_all_data_group_filesRequest"/>
		<output message="tns:LocalLB.DataGroupFile.delete_all_data_group_filesResponse"/>
	</operation>
	<operation name="get_cache_path">
	<documentation>
 Gets the full path to the file inside the file cache for a set of
 data group file objects. For system files outside the cache, like
 ZebOS.conf, the cached path holds a soft link to the external file.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_cache_pathRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_cache_pathResponse"/>
	</operation>
	<operation name="get_checksum">
	<documentation>
 Gets the file checksum, a unique signature used for validation and
 verification for a set of data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_checksumRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_checksumResponse"/>
	</operation>
	<operation name="get_creation_time">
	<documentation>
 Gets the file creation date/time for a set of data group file
 objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_creation_timeRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_creation_timeResponse"/>
	</operation>
	<operation name="get_creator">
	<documentation>
 Gets the name of the user who originally created the file for a set
 of data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_creatorRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_creatorResponse"/>
	</operation>
	<operation name="set_data_separator">
	<documentation>
 Sets the string used to separate a class member value from its
 optional associated data value for a set of data group files.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.set_data_separatorRequest"/>
		<output message="tns:LocalLB.DataGroupFile.set_data_separatorResponse"/>
	</operation>
	<operation name="get_data_separator">
	<documentation>
 Gets the string used to separate a class member value from its
 optional associated data value for a set of data group files

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_data_separatorRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_data_separatorResponse"/>
	</operation>
	<operation name="get_dynamic_state">
	<documentation>
 Gets the state for whether this file can be modified by an external
 process during runtime for a set of data group file objects.
 Setting this allows validation to skip certain steps, such as
 checksum verification.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_dynamic_stateRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_dynamic_stateResponse"/>
	</operation>
	<operation name="set_local_path">
	<documentation>
 Sets the name of the file to be imported into the configuration
 file cache for a set of data group file objects. Specifying this
 attribute for an existing file object triggers importing the
 specified file into the configuration file cache, replacing the
 existing file in the file object. Once the file is imported into
 the file cache, it is deleted. This name must either be a full file
 path or a path relative to /config or /var/class.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.set_local_pathRequest"/>
		<output message="tns:LocalLB.DataGroupFile.set_local_pathResponse"/>
	</operation>
	<operation name="get_modification_time">
	<documentation>
 Gets the date/time of the last file modification for a set of data
 group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_modification_timeRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_modification_timeResponse"/>
	</operation>
	<operation name="get_modifier">
	<documentation>
 Gets the name of the user who last modified the file for a set of
 data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_modifierRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_modifierResponse"/>
	</operation>
	<operation name="get_mode">
	<documentation>
 Gets the bit field holding the file type and permissions, as in the
 Unix "stat" information for a set of data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_modeRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_modeResponse"/>
	</operation>
	<operation name="get_revision">
	<documentation>
 Gets the running count of file modifications for a set of data
 group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_revisionRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_revisionResponse"/>
	</operation>
	<operation name="get_size">
	<documentation>
 Gets the file size for a set of data group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_sizeRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_sizeResponse"/>
	</operation>
	<operation name="get_source_path">
	<documentation>
 Gets the the original source path of the file for a set of data
 group file objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_source_pathRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_source_pathResponse"/>
	</operation>
	<operation name="get_system_path">
	<documentation>
 Gets the full path to system files that exist outside the file
 cache for a set of data group file objects. The cached file is
 simply a soft link to this file.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_system_pathRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_system_pathResponse"/>
	</operation>
	<operation name="get_system_state">
	<documentation>
 Gets the state for whether this is a system file for a set of data
 group file objects. System files cannot be created, deleted, or
 modified by users, and file objects holding them cannot be deleted.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_system_stateRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_system_stateResponse"/>
	</operation>
	<operation name="get_value_type">
	<documentation>
 Gets the data group value type for a set of data group file
 objects.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_value_typeRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_value_typeResponse"/>
	</operation>
	<operation name="get_version">
	<documentation>
 Gets the version information for this interface.

        	</documentation>
		<input message="tns:LocalLB.DataGroupFile.get_versionRequest"/>
		<output message="tns:LocalLB.DataGroupFile.get_versionResponse"/>
	</operation>
</portType>

