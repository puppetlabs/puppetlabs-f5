		</output>
	</operation>
</binding>

<!-- service -->

<service name="iCall.Script">
	<documentation>
 The Script interface allows you to create or delete Tcl scripts to
 attach to handlers.

        	</documentation>
	<port name="iCall.ScriptPort" binding="tns:iCall.ScriptBinding">
		<soap:address location="https://url_to_service"/>
	</port>
</service>
</definitions>
